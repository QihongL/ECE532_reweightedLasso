clear variables; close all; 
load('data/BreastCancer.mat')


[m,n] = size(X);
lambda = 1; 
weights = ones(n,1);

% fit regular lasso vs. reweighed lasso
[beta.std] = lasso_ista(X, y, lambda, weights, 0);
[beta.rw] = reweightedLasso(X, y, lambda, 1);

% find out performance
accuracy.std = sum(sign(X * beta.std) == y )/ length(y)

accuracy.rw = sum(sign(X * beta.rw) == y )/ length(y)

subplot(2,1,1)
plot(beta.std)
title('Beta estimation with standard lasso')
subplot(2,1,2)
plot(beta.rw)